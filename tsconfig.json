{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "NodeNext",
    "module": "commonjs",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["esnext", "DOM"],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "dist"
  }
}